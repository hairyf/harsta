{
  "name": "@harsta/client",
  "version": "0.2.1",
  "description": "Harsta Client is an automatically generated, type safe library for configuring chains, addresses, and contract instances",
  "author": {
    "name": "Hairyf",
    "url": "https://github.com/hairyf"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/hairyf",
  "homepage": "https://github.com/hairyf/harsta",
  "repository": {
    "type": "git",
    "url": "https://github.com/hairyf/harsta/tree/main/packages/client"
  },
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js",
      "require": "./build/index.js"
    },
    "./wagmi": {
      "type": "./build/wagmi/index.d.ts",
      "import": "./build/wagmi/index.js",
      "require": "./build/wagmi/index.js"
    },
    "./chains": {
      "type": "./build/chains/index.d.ts",
      "import": "./build/chains/index.js",
      "require": "./build/chains/index.js"
    },
    "./contracts": {
      "type": "./build/contracts/index.d.ts",
      "import": "./build/contracts/index.js",
      "require": "./build/contracts/index.js"
    },
    "./interfaces": {
      "type": "./build/interfaces/index.d.ts",
      "import": "./build/interfaces/index.js",
      "require": "./build/interfaces/index.js"
    }
  },
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "dependencies": {
    "ethers": "^6.8.1",
    "react": "^18.3.1",
    "viem": "2.x"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "hardhat": "^2.19.0",
    "typescript": "^5.2.2"
  }
}
